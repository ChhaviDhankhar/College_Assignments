ques 2:

#include <bits/stdc++.h>
using namespace std;
 
void permute(string ip, string op)
{
    
    if (ip.size() == 0) {
        cout << op << " ";
        return;
    }
    
    char ch = tolower(ip[0]);
    char ch2 = toupper(ip[0]);
    ip = ip.substr(1);
 
    permute(ip, op + ch);
    permute(ip, op + ch2);
}
int main()
{
    string ip;
    cin >> ip;
    permute(ip, "");
    return 0;
}

