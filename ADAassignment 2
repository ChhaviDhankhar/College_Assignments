ques1:

#include <iostream>
#include <cstring>
using namespace std;

bool ispalindrome(string s){
    int len=s.size();

    int i=0;
    int j=len-1;
    

    while(i<=j){
        if(s[i]==s[j]){
            i++;
            j--;
        }
        else{
            return false;
        }
    }
    return true;
}
int main(){
    string s;
    getline(cin,s);
    if(ispalindrome(s)){
        cout<<"yes"<<endl;
    }
    else{
        cout<<"no"<<endl;
    }

}



